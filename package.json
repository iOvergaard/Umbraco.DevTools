{
  "name": "umbraco.devtools",
  "version": "0.0.1",
  "description": "A browser extension to help with debugging the Webcomponent based Umbraco backoffice V13+",
  "main": "index.js",
  "scripts": {
    "firefox": "web-ext run --source-dir extension/",
    "firefox:developer": "web-ext run --source-dir extension/ --firefox firefoxdeveloperedition",
    "chromium": "web-ext run --source-dir extension/ --target chromium"
  },
  "keywords": [
    "browser-extension",
    "umbraco",
    "webcomponents"
  ],
  "author": "Warren Buckley",
  "private": true,
  "devDependencies": {
    "web-ext": "^7.6.0"
  }
}
